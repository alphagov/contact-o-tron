<h2>All contacts</h2>

<div class="admin-section">
  <ul class="contacts">
    <% for contact in @contacts.in_alphabetical_order %>
      <li><%= link_to contact.name, contact_path(contact) %></li>
    <% end %>
  </ul>
</div>

<div class="format-controls">
  
  <ul>
    <li>
      <li><%= link_to 'Create new contact', new_contact_path, :class => 'button' %></li>
    </li>
  </ul>
  
</div>